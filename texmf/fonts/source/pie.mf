z10=z9+(-dm*1.45dr*2,0); z11=z9+(dm*1.45dr*2,0);
for i=9 upto 11:
  z[i+3]=z[i]+(0,dm*1.4dr*2.8);
  z[i+6]=z[i]+(0,-dm*1.4dr*2.8);
endfor;
for i=9 upto 17:
outersmallpin(z[i],dm*dr,36);
endfor;
cullit;
for i=9 upto 17:
deletering(z[i],dm*dr);
endfor;
cullit;
for i=9 upto 17:
innersmallpin(z[i],dm*dr,36);
endfor;
v19:=currentpicture;
endchar;

beginchar(20, width, height, depth); % 赤伍萬
