cullit;
pickup pencircle scaled 0.02w;
ungray(0,-d,w,h,0.08w);
pickup pencircle scaled 0.15h;
drawdot(0.8w,0.425h);
cullit;
Waku(h,w,d);
v20:=currentpicture;
endchar;

beginchar(21, width, height, depth); % 一萬
